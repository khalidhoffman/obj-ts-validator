  "author": "Khalid Hoffman",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "typson": "^0.6.0"
  }
}
